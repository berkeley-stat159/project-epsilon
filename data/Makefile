download:
	wget http://openfmri.s3.amazonaws.com/tarballs/ds005_raw.tgz
	tar xvzf ds005_raw.tgz

validate:
	wget http://openfmri.s3.amazonaws.com/tarballs/ds005_raw_checksums.txt
	python data.py

